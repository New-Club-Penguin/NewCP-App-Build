{
  "name": "NewCP",
  "productName": "NewCP",
  "version": "1.3.9",
  "description": "NewCP App",
  "author": "Dote",
  "main": "src/index.js",
  "scripts": {
    "start": "electron .",
    "build:win": "electron-builder build --windows",
    "build:mac": "electron-builder build --macos",
    "build:lnx": "electron-builder build --linux"
  },
  "license": "MIT",
  "dependencies": {
    "discord-rich-presence": "^0.0.8",
    "electron-squirrel-startup": "^1.0.0",
    "update-electron-app": "^2.0.1"
  },
  "devDependencies": {
    "electron": "12.0.15",
    "electron-builder": "^23.0.3"
  },
  "build": {
    "appId": "net.newcp.newcp",
    "extraResources": [
      {
        "from": "./lib/",
        "to": "lib",
        "filter": [
          "**/*"
        ]
      }
    ],
    "win": {
      "target": "nsis",
      "icon": "build/icons/icon.ico"
    },
    "mac": {
      "target": ["dmg", "zip"],
      "icon": "build/icons/icon.icns"
    },
    "linux": {
      "icon": "build/icons/icon.png",
      "category": "Games",
      "synopsis": "NewCP App",
      "description": "NewCP App"
    },
    "protocols": [
      {
        "name": "newcp",
        "schemes": [
          "newcp"
        ]
      }
    ]
  }
}
